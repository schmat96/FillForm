{
  "name": "FillForm",
  "version": "1.0",
  "description": "Fill your Forms automatically",
  "permissions": ["tabs", "<all_urls>"],
  "web_accessible_resources": [
    "script/main.js"
  ],
  "browser_action": {
    "js": ["popup.js"],
    "default_popup": "popup.html",
    "default_title": "Fill Form",
    "default_icon": {
      "16": "icons/Icon-16.png",
      "32": "icons/Icon-32.png",
      "48": "icons/Icon-16.png",
      "128": "icons/Icon-128.png"
    }
  },
  "icons": {
    "16": "icons/Icon-16.png",
    "32": "icons/Icon-32.png",
    "48": "icons/Icon-16.png",
    "128": "icons/Icon-128.png"
  },
  "manifest_version": 2
}